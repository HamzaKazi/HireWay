<div class="container">
            <div class="text-center mt-5">
                <h1>My Listings</h1>
          </section>
            </div>
<div class="box-index" >
    <div class="container">
      <div class="row justify-content-center" >
          <% @vehicles.each do |vehicle| %>
          <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12" style="width: 18rem;">
          <div class="box-part text-center">
          <%=cl_image_tag vehicle.image.key, class: "img-mx-auto d-block card-img-top img-fluid" %>
            <div class="title">
                        <br>
                        <br>
              <h4><%= vehicle.name%></h4>
            </div>
            <div class="text">
              <span><%= vehicle.address%></span>
            </div>
          <%=  link_to "View", vehicle_path(vehicle), class: "btn btn-cancel"%>
           </div>
        </div>
            <% end %>
