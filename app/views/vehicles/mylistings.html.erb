
<% @vehicles.each do | vehicle | %>
<%= vehicle.name %>
<% end %>



<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" />

<div class="container">
        <div class="row justify-content-center">
          <div class="col-12 col-sm-8 col-lg-6">
            <!-- Section Heading-->
            <div class="section_heading text-center wow fadeInUp" data-wow-delay="0.2s" style="visibility: visible; animation-delay: 0.2s; animation-name: fadeInUp;">
              <h3>Your listings <span> Team</span></h3>
              <p>Pgfdg</p>
              <div class="line"></div>
            </div>
          </div>
        </div>
          <!-- Single Advior-->
        <div class="row">
          <% @vehicles.each do | vehicle | %>
          <div class="col-10 col-sm-12 col-lg6 d-flex">
            <div class="single_advisor_profile wow fadeInUp" data-wow-delay="0.2s" style="visibility: visible; animation-delay: 0.2s; animation-name: fadeInUp;">
              <!-- Image-->
              <div class="advisor_thumb"><%= cl_image_tag vehicle.image.key %>
                  <button> <%=  link_to "DELETE", vehicle_path(vehicle.id), class: "btn btn-cancel", data: { turbo_method: :delete, turbo_confirm: "Are you sure you want to delete this list?" } %> </button>

              <!-- Vehicle Details-->
              <div class="single_advisor_details_info">
                <h6><%= vehicle.name %></h6>
                <p class="designation">sdcd &amp; cdsc</p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  <% end %>
</div>
