        <div class="panel">

          <h2 style="color: #ff6969; font-size: 25px;"><strong>Add your vehicle</strong></h2>

        </div>

       <%= simple_form_for @vehicle do |f| %>

        <div class="row" style="margin-top: 2%;">

          <div class="col-sm-12 col-md-6 form-group">
          <%= f.input :name, placeholder: "BMW, Mercedes..." %>

          </div>

        <div class="row" style="margin-top: 2%;">


          <div class="col-sm-12 col-md-6 form-group">
            <%= f.input :category, label: "Enter vehicle category", collection: ["Aircraft", "Yacht", "Limousine", "Car"] %>
          </div>

        </div>

        <div class="row" style="margin-top: 2%;">

          <%# <div class="col-md-4 form-group">
            <label for="category">Vehicle category, please select</label>
            <select class="form-control" id="category" name="category">
